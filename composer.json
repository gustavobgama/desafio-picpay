{
    "name": "gustavobgama/desafio-picpay",
    "description": "API de cadastro de Usu√°rios e controle de Pagamentos",
    "type": "project",
    "require": {
        "php": "^7.2",
        "laravel/lumen-framework": "^6.0",
        "guzzlehttp/guzzle": "^6.0",
        "pearl/lumen-request-validate": "1.4.*"
